{
  "env": {
    "node": true,
    "commonjs": true
  },
  "extends": ["airbnb", "prettier"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2015,
    "ecmaFeatures": {
      "jsx": true
    },
    "requireConfigFile": false
  },

  "rules": {
    "semi": ["error", "always"],
    "quotes": "off",
    "no-console": "off",
    "no-redeclare": "warn",
    "import/no-unresolved": "off",
    "import/prefer-default-export": "off"
  }
}
